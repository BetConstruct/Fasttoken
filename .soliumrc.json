{
        "extends": "solium:all",
        "plugins": [ "security" ],
        "rules": {
                "lbrace": 0,
                "quotes": [ "error", "single" ],
                "indentation": [ "error", 8 ],

                "security/max-statements-in-func": ["warning"],
                "security/no-block-members": 0
        }
}
